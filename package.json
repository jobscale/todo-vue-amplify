    "@vue/compiler-sfc": "^3.2.20",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.0.0",
    "serve": "^12.0.1"
  },
  "eslintConfig": {
    "root": true,
